 <input\n                        type=\"text\"\n                        name=\"description\"\n                        placeholder=\"Description\"\n                        onChange={this.onChangeDescription}\n                        value={description}\n                    />\n                </div>\n                <div className=\"input\">\n                    <input\n                        type=\"date\"\n                        name=\"date\"\n                        min=\"01/01/2021\"\n                        max=\"01/01/2030\"\n                        onChange={this.onChangeDate}\n                        value={date}\n                    />\n                </div>\n                <div className=\"input\">\n                    <input\n                        type=\"text\"\n                        name=\"imageURL\"\n                        placeholder=\"imageURL\"\n                        onChange={this.onChangeImageURL}\n                        value={imageURL}\n                    />\n                </div>\n                <div className=\"input\">\n                    <input\n                        type=\"text\"\n                        name=\"location\"\n                        placeholder=\"Location\"\n                        onChange={this.onChangeLocation}\n                        value={location}\n                    />\n                </div>\n                <button type=\"submit\" className=\"btn\">CREATE</button>\n            </form>\n        )\n    }\n}*/\n\nexport default Create;","/Users/kishank/Desktop/Event-Manager/client/src/components/events/edit/Edit.js",[],"/Users/kishank/Desktop/Event-Manager/client/src/components/events/details/Details.js",["88"],"import React, {useState, useEffect} from \"react\";\n\nimport \"./Details.css\";\nimport {useParams, useHistory} from \"react-router\";\nimport isAdmin from \"../../../utils/isAdmin\";\nimport isLiked from \"../../../utils/isLiked\";\nimport eventServices from \"../../../services/event-services\